{
  "name": "voting-system-backend",
  "version": "1.0.0",
  "description": "Backend for the Indian Voting Management System",
  "main": "dist/server.js",
  "scripts": {
    "start": "node dist/server.js",
    "dev": "ts-node-dev --respawn --transpile-only src/server.ts",
    "build": "tsc",
    "test": "jest",
    "lint": "eslint . --ext .ts",
    "format": "prettier --write \"src/**/*.ts\"",
    "db:create": "psql -U postgres -h localhost -p 5432 -f src/db/create_db.sql",
    "db:migrate": "psql -U postgres -h localhost -p 5432 -d voting_system -f src/db/schema.sql",
    "db:setup": "npm run db:create && npm run db:migrate",
    "download:models": "ts-node src/utils/downloadModels.ts"
  },
  "keywords": [
    "voting",
    "election",
    "queue-management",
    "face-recognition",
    "qr-code"
  ],
  "author": "",
  "license": "ISC",
  "dependencies": {
    "@emotion/react": "^11.14.0",
    "@emotion/styled": "^11.14.0",
    "@mui/icons-material": "^6.4.8",
    "@mui/material": "^6.4.8",
    "@types/react-webcam": "^1.1.0",
    "axios": "^1.8.4",
    "bcrypt": "^5.1.1",
    "bcryptjs": "^2.4.3",
    "cors": "^2.8.5",
    "dotenv": "^16.0.3",
    "express": "^4.18.2",
    "jsonwebtoken": "^9.0.0",
    "pg": "^8.10.0",
    "qrcode": "^1.5.0",
    "react-router-dom": "^7.4.0",
    "react-webcam": "^7.2.0",
    "socket.io": "^4.6.1"
  },
  "devDependencies": {
    "@types/bcryptjs": "^2.4.2",
    "@types/cors": "^2.8.13",
    "@types/express": "^4.17.17",
    "@types/jsonwebtoken": "^9.0.1",
    "@types/node": "^18.15.11",
    "@types/pg": "^8.6.6",
    "@types/qrcode": "^1.5.0",
    "@types/socket.io": "^3.0.2",
    "@typescript-eslint/eslint-plugin": "^5.58.0",
    "@typescript-eslint/parser": "^5.58.0",
    "@vitejs/plugin-react": "^4.3.4",
    "eslint": "^8.38.0",
    "prettier": "^2.8.7",
    "ts-node-dev": "^2.0.0",
    "typescript": "^5.0.4"
  }
}
